    "concurrently": "^5.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "flush-promises": "^1.0.2",
    "nodemon": "^2.0.6",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
