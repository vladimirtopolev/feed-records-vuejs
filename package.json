    "server": "nodemon server/index.js",
    "start": "concurrently --kill-others \"npm run server\" \"npm run serve\""
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "node-cron": "^2.0.3",
    "uuid": "^8.3.1",
    "vue": "^2.6.11",
    "vue-i18n": "^8.22.2",
    "vue-router": "^3.4.9",
    "vuetify": "^2.2.11",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@testing-library/user-event": "^12.4.0",
    "@testing-library/vue": "^5.6.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.9",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.0.3",
